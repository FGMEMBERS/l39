<PropertyList>
<sim>
	<description>Aero Vodochody L-39 Albatros</description>
	<author>Fabrice KAUFFMANN, Stuart C</author>
	<status>alpha 1.8b</status>
	<flight-model>yasim</flight-model>
	<aero>l39-yasim</aero>	

	<startup>
		<splash-texture>Aircraft/l39/l39-splash.png</splash-texture>
	</startup>
	
	<model>
		<path>Aircraft/l39/Models/l39.xml</path>
		
		<hide-lights type="int">1</hide-lights>	
            <livery>  
				<name type="string">Default</name>
				<texture>default.png</texture>
                                              
	      </livery>

	</model>
	
	<rating>
<FDM type="int">4</FDM>
<systems type="int">2</systems>
<cockpit type="int">3</cockpit>
<model type="int">3</model>
</rating>
	
	<fuel-fraction>1.0</fuel-fraction>
	
	
	<submodels>
      <serviceable type="bool">true</serviceable>
      <path>Aircraft/l39/Models/stores/loads.xml</path> 
      <path>Aircraft/l39/Models/stores/pylons.xml</path>	  
	</submodels>
	
	
	<sound>
		<audible>true</audible>
		<path>Aircraft/l39/Sounds/l39-sound.xml</path>
	</sound>
	
	<systems>
     <autopilot>
         <path>Aircraft/l39/Systems/autopilot5.xml</path>
      </autopilot>
    <electrical>
         <path>Aircraft/Generic/generic-electrical.xml</path>
    </electrical>	  
	  
      
    </systems> 
	
 <help>
   <title>L-39</title>
    <key>
     <name>}</name>
     <desc>Start/Stop Engine</desc>
   </key>
  
   <key>
     <name>Ctrl+b</name>
     <desc>Airbrake Toggle</desc>
   </key>
   <line> Max takeoff weight: 4,700 kg (10,362 lb)</line>
	<line>Maximum speed: 750 km/h (405 knots, 470 mph) at 4,000 m (16,000 ft)</line>
	
  </help>	

<hud>
      <path n="1">Aircraft/l39/Hud/hud.xml</path>
	  
      <visibility n="0" type="bool">false</visibility>
      <visibility n="1" type="bool">true</visibility>
      <current-color>0</current-color>
      <color>
	<transparent type="bool">true</transparent>
	<antialiased type="bool">true</antialiased>
	<brightness type="float">1.0</brightness>
	<alpha type="float">1.0</alpha>
      </color>
      <palette>
	<color n="0">
	  <red type="float">0.00</red>
	  <green type="float">1.0</green>
	  <blue type="float">0.00</blue>
	</color>
	<color n="1">
	  <red type="float">1.0</red>
	  <green type="float">0.0</green>
	  <blue type="float">0.0</blue>
	</color>
    <color n="2">
	  <red type="float">0.0</red>
	  <green type="float">0.0</green>
	  <blue type="float">1.0</blue>
	</color>
    <color n="3">
	  <red type="float">1.0</red>
	  <green type="float">1.0</green>
	  <blue type="float">1.0</blue>
	</color>
      </palette>
	 
  <clipping>
    <left type="double">-75</left>
    <right type="double">70</right>
    <top type="double">-35</top>
    
	  <bottom type="double">-165</bottom>
     </clipping>
	
    </hud>
	
	<virtual-cockpit archive="y">true</virtual-cockpit>
	
	<chase-distance-m type="double" archive="y">-20.0</chase-distance-m>
	
	<menubar>
		<default>
			<menu n="99">
				<label>Albatros</label>
				<enabled type="bool">true</enabled>
				
				<item>
					<label>Switch Lights On/Off</label>
					<binding>
						<command>property-toggle</command>
						<property>/sim/model/hide-lights</property>
					</binding>
				</item>	
                        <item>
				<label>Select Livery</label>
				<binding>
					<command>nasal</command>
					<script>aircraft.livery.dialog.open()</script>
				</binding>
			      </item>
				
			</menu>
		</default>
	</menubar>
	
	<view n="0">
		<internal archive="y">true</internal>
		<config>
			<x-offset-m archive="y">0.00</x-offset-m>
			<y-offset-m archive="y">-0.1</y-offset-m>
			<z-offset-m archive="y">-2.75</z-offset-m>
			<pitch-offset-deg>0</pitch-offset-deg>
			<default-field-of-view-deg>65</default-field-of-view-deg>
		</config>
	</view>
	
	 <view n="100">
  <name>Instructors View</name>
  <internal archive="y">true</internal>
	<type>lookfrom</type>
	<config>
	<from-model type="bool">true</from-model>
	<from-model-idx type="int">0</from-model-idx>
    <x-offset-m archive="y">0.00</x-offset-m>
			<y-offset-m archive="y">0.09</y-offset-m>
			<z-offset-m archive="y">-1.38</z-offset-m>
	<limits>
	   <left>
	  <heading-max-deg>-100</heading-max-deg>
<x-offset-threshold-deg>0</x-offset-threshold-deg>
<x-offset-max-m>0</x-offset-max-m>
</left>
<right>
<heading-max-deg>100</heading-max-deg>
<x-offset-threshold-deg>0</x-offset-threshold-deg>
<x-offset-max-m>0</x-offset-max-m>
</right>
<enabled>1</enabled>
</limits>
   </config>
  </view>
  
   <view n="101">
