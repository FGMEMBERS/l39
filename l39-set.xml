</sim>

  <environment>
    <turbulence>
      <use-cloud-turbulence>true</use-cloud-turbulence>
    </turbulence>
  </environment>

<controls>
	<lighting>
		<strobe type="bool">true</strobe>
	</lighting>
	<gear>
		<brake-parking type="double">1.0</brake-parking>
	</gear>  
	
		
<armament>
