<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>pylons.ac</path>
 <animation>
    <object-name>pylonL1</object-name>
    <object-name>pylonL2</object-name>
    <object-name>pylonC</object-name>
    <object-name>pylonR2</object-name>
    <object-name>pylonR1</object-name>
  </animation>

<!--station left 1 -->
  <animation>
    <type>select</type>
    <object-name>pylonL1</object-name>      
    <condition>
      <not-equals>
        <property>sim/weight[0]/selected</property>
        <value>none</value>
      </not-equals>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>pylonL2</object-name>      
    <condition>
      <not-equals>
        <property>sim/weight[1]/selected</property>
        <value>none</value>
      </not-equals>
    </condition>
  </animation>
  
  <animation>
    <type>select</type>
    <object-name>pylonC</object-name>      
    <condition>
      <not-equals>
        <property>sim/weight[2]/selected</property>
        <value>none</value>
      </not-equals>
    </condition>
  </animation>

  

  <animation>
    <type>select</type>
    <object-name>pylonR2</object-name>      
    <condition>
      <not-equals>
        <property>sim/weight[3]/selected</property>
        <value>none</value>
      </not-equals>
    </condition>
  </animation>
  
  <animation>
    <type>select</type>
    <object-name>pylonR1</object-name>      
    <condition>
      <not-equals>
        <property>sim/weight[4]/selected</property>
        <value>none</value>
      </not-equals>
    </condition>
  </animation>

 
		

</PropertyList>
